[user]
  name = ogaclejapan
  email = ogaclejapan@gmail.com
[color]
  ui = auto
[core]
  excludesfile = ~/.gitignore
  autocrlf = input
[alias]
  # log
  tree = log --graph --all --color --pretty='%x09%h %cn%x09%s %Cred%d%Creset'
  # diff
  dm = diff master
  d1 = diff HEAD~
  d2 = diff HEAD~~
  d3 = diff HEAD~~~
  d4 = diff HEAD~~~~
  d5 = diff HEAD~~~~~
  d10 = diff HEAD~~~~~~~~~~
  # delete unused local branches
  diet = !git branch --merged master | grep -vE \"^\\*|master$\" | xargs -I % git branch -d %
[gh]
  #host = your-github-enterprise
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[mergetool]
	keepBackup = false
[fetch]
	prune = true
[rebase]
	autosquash = true
	autostash = true
