[user]
  name = ogaclejapan
  email = ogaclejapan@gmail.com
[color]
  ui = auto
[core]
  editor = vim
  excludesfile = ~/.gitignore
  autocrlf = input
[alias]
  # log
  oneline = log --pretty=oneline
  log-all = log --graph --all --color --pretty='%x09%h %cn%x09%s %Cred%d%Creset'
  # diff
  dm = diff master
  d1 = diff HEAD~
  d2 = diff HEAD~~
  d3 = diff HEAD~~~
  d4 = diff HEAD~~~~
  d5 = diff HEAD~~~~~
  d10 = diff HEAD~~~~~~~~~~
[gh]
  #host = your-github-enterprise
