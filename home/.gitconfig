[user]
  name = ogaclejapan
  email = ogaclejapan@gmail.com
[color]
  ui = auto
[core]
  excludesfile = ~/.gitignore
  autocrlf = input
[alias]
  l = log --oneline
  ll = log --stat
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
  a = add
  ap = add -p
  co = checkout
  d = diff
  dc = diff --cached
  dm = diff master
  d1 = diff HEAD~
  d2 = diff HEAD~~
  d3 = diff HEAD~~~
  d4 = diff HEAD~~~~
  d5 = diff HEAD~~~~~
  pr = pull --rebase
  cv = commit --verbose
  ci = commit
  ca = commit -am
  s = status -s
  br = branch
  master = checkout master
  diet = !git branch --merged master | grep -vE \"^\\*|master$\" | xargs -I % git branch -d %
[gh]
  #host = your-github-enterprise
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	required = true
	process = git-lfs filter-process
[mergetool]
	keepBackup = false
[fetch]
	prune = true
[rebase]
	autosquash = true
	autostash = true
[giter]
  root = ~/git
[github]
	user = ogaclejapan
