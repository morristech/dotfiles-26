  <!-- 
    Specify application settings
   -->

  <appdef>
    <appname>ECLIPSE</appname>
    <equal>org.eclipse.platform.ide</equal>
  </appdef>
  <appdef>
    <appname>INTELLI_J</appname>
    <equal>com.jetbrains.intellij</equal>
  </appdef>
  <appdef>
    <appname>ANDROID_STUDIO</appname>
    <equal>com.google.android.studio</equal>
  </appdef>

  <item>
    <name>Space( ␣ ) + SemiColon( ; ) to Code Completion for IDE</name>
    <appendix>アプリケーション(Eclipse, IntelliJのみ) - コード補完(Ctrl+Space)</appendix>  
    <identifier>ogaclejapan.remap_code_completion_for_ide</identifier>
    <only>ECLIPSE, INTELLI_J, ANDROID_STUDIO</only>
    <autogen>
      __KeyOverlaidModifier__
      KeyCode::SPACE,
      KeyCode::VK_MODIFIER_EXTRA1,
      KeyCode::SPACE
    </autogen>
    <autogen>
      __KeyToKey__
      KeyCode::SEMICOLON, ModifierFlag::EXTRA1, 
      KeyCode::SPACE, ModifierFlag::CONTROL_L
    </autogen>
  </item>

  <item>
    <name>Space( ␣ ) + SemiColon( ; ) to Prefix key for tmux</name>
    <appendix>アプリケーション(tmuxのみ) - プレフィックスキー(Ctrl+b)</appendix>  
    <identifier>ogaclejapan.remap_prefix_key_for_tmux</identifier>
    <only>TERMINAL</only>
    <autogen>
      __KeyOverlaidModifier__
      KeyCode::SPACE,
      KeyCode::VK_MODIFIER_EXTRA1,
      KeyCode::SPACE
    </autogen>
    <autogen>
      __KeyToKey__
      KeyCode::SEMICOLON, ModifierFlag::EXTRA1, 
      KeyCode::B, ModifierFlag::CONTROL_L
    </autogen>
  </item>

